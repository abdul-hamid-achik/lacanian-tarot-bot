{
    "crons": [
        {
            "path": "/api/cron/theme-decay",
            "schedule": "0 0 * * *"
        }
    ],
    "buildCommand": "next build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "regions": [
        "iad1"
    ],
    "builds": [
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        }
    ],
    "env": {
        "NEXT_PUBLIC_VERCEL_ENV": "production"
    },
    "git": {
        "deploymentEnabled": {
            "main": true
        }
    }
}
